{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "/",
	"type": "object",
	"additionalProperties": false,
	"required": [
		"VehiclePlateNumber", "TagNumber"
	],
	"properties": {
		"VehiclePlateNumber": {
			"type": "string",
			"pattern": "^[a-zA-Z0-9]*$",
			"maxLength": 12
		},
		"TagNumber": {
			"type": "string",
			"pattern": "^[a-z_A-Z]{2}\\d{6}$|^\\d{8}$",
			"example": "AB123456"
		}
	},
	"definitions": {
		"TicketQueryResp": {
			"type": "object",
			"properties": {
				"Service": {
					"$ref": "#/definitions/TicketService"
				},
				"Payables": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/TicketPayable"
					}
				},
				"validateResponse" : {
					"type": "object",
					"properties": {
						"status": {
							"type": "string",
							"example": "FAILURE"
						},
						"tagNumber": {
							"type": "string",
							"example": "NR375822"
						},
						"plateNumber": {
							"type": "string",
							"example": "BYJB503"
						},
						"errorLabel": {
							"type": "string",
							"example": "TAG_NUMBER"
						},
						"errorMessage": {
							"type": "string",
							"example": "You must provide a valid tag number."
						},
						"errorCode": {
							"type": "string",
							"example": "ERR-RSD-VAL-008"
						}
					}
				}
			},
			"description": "Parking Ticket payables"
		},
		"TicketService": {
			"type": "object",
			"properties": {
				"Type": {
					"type": "string",
					"example": "PARKINGTAGS",
					"enum": [
						"PARKINGTAGS"
					]
				},
				"VehiclePlateNumber": {
					"type": "string",
					"example": "ABCD234"
				}
			}
		},
		"TicketPayable": {
			"type": "object",
			"properties": {
				"Type": {
					"type": "string",
					"example": "ParkingTicket"
				},
				"AmountDue": {
					"type": "number",
					"format": "double",
					"example": 15.12
				},
				"DueDate": {
					"type": "string",
					"format": "date",
					"example": "2019-11-01"
				},
				"TagNumber": {
					"type": "string",
					"pattern": "^[a-z_A-Z]{2}\\d{6}$|^\\d{8}$",
					"example": "AB123456"
				},
				"NSFRefNo": {
					"type": "string",
					"example": "sadf3"
				}
			}
		}
	}
}
